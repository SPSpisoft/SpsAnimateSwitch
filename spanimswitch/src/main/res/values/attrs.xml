<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SpaSwitch">
        <attr name="SwitchSize" format="dimension"/>
        <attr name="TitleShow" format="boolean"/>
        <attr name="TitleMain" format="string"/>
        <attr name="TextColor" format="color"/>
        <attr name="AnimateDuration" format="integer"/>
        <attr name="SrcBackground" format="reference"/>
        <attr name="SetDefault" format="boolean"/>
        <attr name="AnimateType">
            <enum name="TranslateSlide" value="0" />
            <enum name="TopToBottom" value="1" />
        </attr>
    </declare-styleable>

</resources>